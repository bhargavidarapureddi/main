    "# Find the area of triangle\n",
    "\n",
    "a = 7\n",
    "b = 9\n",
    "c = 12\n",
    "\n",
    "# calculate the semi-perimeter\n",
    "s = (a + b + c) / 2\n",
    "\n",
    "area = (s*(s-a)*(s-b)*(s-c)) ** 0.5\n",
    "print(area)"
   ]
