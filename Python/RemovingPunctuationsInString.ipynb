    "import re\n",
    "\n",
    "def remove_punctuation(s):\n",
    "    return re.sub(r'[^\\w\\s]', '', s)\n",
    "\n",
    "text = \"Hello,.... this is -----Bhargavi! \"\n",
    "clean_text = remove_punctuation(text)\n",
    "\n",
    "print(\"Original string:\", text)\n",
    "print(\"String without punctuation:\", clean_text)\n"
   ]
  }
